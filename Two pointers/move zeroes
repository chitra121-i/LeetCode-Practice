//Leetcode 283 
//MOVE ZEROES
class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int i;
        int j;
        int n=nums.size();
        for(i=0;i<n;i++){
            for(j=i+1;j<n;j++){
                if(nums[i]==0){
                    if(nums[j]!=0){
                        swap(nums[i],nums[j]);
                    }
                }
            }
        }
    }
};
